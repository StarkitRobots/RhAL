{
    "name": "starkit/rhal",
    "deps": [
        "starkitdeps/serial", 
        "starkitdeps/json", 
        "starkitdeps/tclap", 
        "starkit/rhio",
        "starkitdeps/eigen"
    ],
    "includes": "Lib",
    "binaries": "build/bin",
    "links": "build/libRhAL.so",
    "build": [
        "mkdir -p build",
        "cd build",
        "cmake -DCMAKE_BUILD_TYPE=Release -DBUILD_RHAL_BINARY=ON ..",
        "make"
    ]
}
